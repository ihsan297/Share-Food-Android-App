<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="2dip" />
            <stroke android:width="1dip" android:color="#6FEE15" />
            <gradient android:angle="-90" android:startColor="#65E60B" android:endColor="#72F119"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="2dip" />
            <stroke android:width="1dip" android:color="#70FD0D" />
            <solid android:color="#69EB0E"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="2dip" />
            <stroke android:width="1dip" android:color="#63DD0D" />
            <gradient android:angle="-90" android:startColor="#F8F7F5" android:endColor="#0ECC17" />
        </shape>
    </item>
</selector>